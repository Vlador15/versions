{
  "name": "project",
  "version": "1.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "release": "standard-version -m ':bookmark: Release %s'",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version": "standard-version -m ':bookmark: Version %s'",
    "version:patch": "npm run version -- --release-as patch",
    "version:minor": "npm run version -- --release-as minor",
    "version:major": "npm run version -- --release-as major"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "commitizen": "^4.3.0",
    "conventional-changelog": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "standard-version": "^9.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
